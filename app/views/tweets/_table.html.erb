<table class="highlight table-color">
  <thead>
    <tr>
        <th>State</th>
        <th>Bernie Tweets</th>
        <th>Trump Tweets</th>
        <th>Winner's Margin</th>
    </tr>
  </thead>

  <tbody>
    <% @twitter.states.each do |state| %>
      <tr>
        <td>
          <%= link_to "#{state}", tweet_path(:id), class: "waves-effect waves-light btn indigo darken-2" %>
        </td>
        <td>
          <%= state %>
        </td>
        <td>
          <%= state %>
        </td>
        <td>
          <%= state %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
